/* pages/newAnalysis/newAnalysis.wxss */
page{
  padding: 0;
}
.container .swiper-box {
  background: #fff;
  padding: 40rpx 0 10rpx;
  margin: 0 0 18rpx;
}

.container .swiper {
  width: 720rpx;
  height: 400rpx;
  margin: 0 auto;
}

.container .swiper-box image {
  width: 720rpx;
}


.main{
  background: #fff;
}
.container .h2 {
  padding: 30rpx 76rpx 20rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  font-weight: 500;
  border-bottom: 1px solid #EAEBF1;
}
.container .box{
  padding: 26rpx 46rpx 36rpx;
  display: flex;
  flex-wrap: wrap;
}
.container .box .item{
  width: 33.33%;
  text-align: center;
  line-height: 40rpx;
  font-size: 28rpx;
  padding: 0 0 48rpx;
  box-sizing: border-box;
  border-right: 1px solid #EAEBF1;
  border-bottom: 1px solid #EAEBF1;
}
.container .box .item:nth-child(3n){
  border-right: 0;
}
.container .box .item:nth-last-child(2),
.container .box .item:last-child{
  border-bottom: 0;
}
.container .box image{
  width: 52rpx;
  margin: 30rpx 0 20rpx;
  display: inline-block;
}

.guide{
  padding: 0 0 120rpx;
  background: #d1e3fd;
}
.guide .swiper {
  padding: 0 20rpx;
  height: 1400rpx;
}
.guide .swiper .h2{
  font-size: 30rpx;
  line-height: 90rpx;
  color: #2f77d6;
}
.guide .swiper image{
  width: 100%;
}
.guide .button{
  position: absolute;
  bottom: 26%;
  left: 20%;
  width: 60%;
  text-align: center;
  border-radius: 42rpx;
  min-width: auto;
}
.guide .link{
  position: absolute;
  right: 20rpx;
  top: 1365rpx;
  color: #2f77d6;
  font-size: 18rpx;
}