/* pages/figure1/figure1.wxss */
page{
  background: #fff;
}

.table {
  margin:26rpx 0 0;
}
.tr{
  padding: 0 44rpx;
}
.table .th:first-child,
.table .td:first-child {
  width: 11%;
}
.table .th:nth-child(2),
.table .td:nth-child(2){
  width: 38%;
  text-align: left;
}
.table .th:nth-child(2){
  text-indent: 3em;
}
.table .th:nth-child(3),
.table .td:nth-child(3){
  width: 24%;
}
.table .th:nth-child(4),
.table .td:nth-child(4) {
  width: 15%;
}
.table .th:last-child,
.table .td:last-child {
  width: 12%;
}
.table image{
  width: 20rpx;
  height: 20rpx!important;
}